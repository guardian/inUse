
# front end
GET     /                             controllers.IndexController.index

# register a new service that may be called
PUT /api/v1/services/:service         controllers.APIController.addService(service: String)

# add a new call against the service
POST /api/v1/services/:service/records        controllers.APIController.addRecord(service: String)

## get list of services
#GET /api/v1/services/                 controllers.APIController.index
#
## get list of records against service
#GET /api/v1/services/:service         controllers.APIController.index
#
## get record against service
#GET /api/v1/services/:service/records controllers.APIController.index


GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)
